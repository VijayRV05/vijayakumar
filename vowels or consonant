alphabet=raw_input("alphabet");
if (alphabet =="a"):
    print "the number is vowel"
elif (alphabet =="e"):
    print "the number is vowel"
elif (alphabet =="i"):
    print "the number is vowel"
elif (alphabet =="o"):
    print "the number is vowel"
elif (alphabet =="u"):
   print "the number is vowel"
else:
    print "the number is consonant"
